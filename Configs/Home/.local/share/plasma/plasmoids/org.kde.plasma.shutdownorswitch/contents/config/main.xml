        <default>true</default>
    </entry>
    <entry name="showShutdown" type="Bool">
        <default>true</default>
    </entry>
    <entry name="showSuspend" type="Bool">
        <default>false</default>
    </entry>
    <entry name="showHybernate" type="Bool">
        <default>false</default>
    </entry>
    <entry name="showLogOut" type="Bool">
